<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.Gcc.Deadeye.Fragment.VeitnamFragment">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/blue">

        <ImageView
            android:id="@+id/taptoactivatevn"
            android:layout_width="134dp"
            android:layout_height="449.71px"
            android:layout_marginStart="127dp"
            android:layout_marginTop="40dp"
            android:layout_marginEnd="132dp"
            android:layout_marginBottom="34dp"
            android:src="@drawable/logo"
            app:layout_constraintBottom_toTopOf="@+id/relativeLayout3"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.0" />

        <RelativeLayout
            android:id="@+id/relativeLayout3"
            android:layout_width="296dp"
            android:layout_height="262dp"
            android:layout_marginTop="5dp"
            android:layout_marginBottom="92dp"
            android:background="@drawable/shapebelowbtns"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.495"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/taptoactivatevn">


            <Button
                android:id="@+id/stopcheatvn"
                android:layout_width="111dp"
                android:layout_height="36dp"
                android:layout_alignParentTop="true"
                android:layout_alignParentEnd="true"
                android:layout_marginTop="20dp"
                android:layout_marginEnd="25dp"
                android:background="@drawable/stop_cheat_btn"
                android:text="Stop Cheat"
                android:textColor="@color/black"
                android:textSize="12dp"
                android:textStyle="bold"
                tools:layout_editor_absoluteX="240dp"
                tools:layout_editor_absoluteY="365dp" />

            <Button
                android:id="@+id/startcheatvn"
                android:layout_width="109dp"
                android:layout_height="36dp"
                android:layout_alignParentTop="true"
                android:layout_marginTop="20dp"
                android:layout_marginEnd="22dp"
                android:layout_toStartOf="@+id/stopcheatvn"
                android:background="@drawable/startcheatbtn"
                android:text="Start Cheat"
                android:textColor="@color/black"
                android:textSize="12dp"
                tools:layout_editor_absoluteX="53dp"
                tools:layout_editor_absoluteY="370dp" />

            <Button
                android:id="@+id/veitnamfixgame"
                android:layout_width="252dp"
                android:layout_height="44dp"
                android:layout_alignParentStart="true"
                android:layout_alignParentEnd="true"
                android:layout_alignParentBottom="true"
                android:layout_marginStart="25dp"
                android:layout_marginEnd="25dp"
                android:layout_marginBottom="19dp"
                android:background="@drawable/fixguestbutton"
                tools:layout_editor_absoluteX="53dp"
                tools:layout_editor_absoluteY="544dp" />

            <Button
                android:id="@+id/veitnamcleanguest"
                android:layout_width="252dp"
                android:layout_height="44dp"
                android:layout_below="@+id/stopcheatvn"
                android:layout_alignParentStart="true"
                android:layout_alignParentEnd="true"
                android:layout_marginStart="25dp"
                android:layout_marginTop="18dp"
                android:layout_marginEnd="25dp"
                android:background="@drawable/cleanguestfullbtn"
                tools:layout_editor_absoluteX="56dp"
                tools:layout_editor_absoluteY="465dp" />

            <Button
                android:id="@+id/deepfixgamevn"
                android:layout_height="44dp"
                android:layout_alignParentStart="true"
                android:layout_alignParentEnd="true"
                android:layout_alignParentBottom="true"
                android:layout_marginStart="25dp"
                android:layout_marginEnd="25dp"
                android:layout_marginBottom="82dp"
                android:background="@drawable/deepcleanfix"
                android:layout_width="252dp" />


        </RelativeLayout>

    </androidx.constraintlayout.widget.ConstraintLayout>


</FrameLayout>